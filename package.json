{
  "name": "quickly",
  "version": "0.1.2",
  "private": true,
  "author": "alan <langwdalan@gmail.com>",
  "main": "main.js",
  "homepage": ".",
  "build": {
    "appId": "com.alan.quickly",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*",
      "main.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win":{
      "icon": "public/favicon.ico"
    }
  },
  "dependencies": {
    "antd": "^3.12.1",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "fs": "^0.0.1-security",
    "mousetrap": "^1.6.2",
    "nedb": "^1.8.0",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "2.0.2-next.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "react": "react-app-rewired start",
    "electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .",
    "start": "concurrently \"yarn react\" \"yarn electron\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "package": "yarn build && electron-builder -c.extraMetadata.main=main.js --win --x64"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "License": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^15.5.5",
    "electron-builder": "^20.38.4",
    "wait-on": "^3.2.0"
  }
}
